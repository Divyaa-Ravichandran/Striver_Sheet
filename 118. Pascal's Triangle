

URL: https://leetcode.com/problems/pascals-triangle/description/




class Solution:
    def generate(self, numRows: int) -> List[List[int]]:

        ans=[]
        for row in range(1,numRows+1):
            ans.append(self.pascal(row))

        return ans

    def pascal(self,row):
        ans=1
        ansrow=[1]
        for col in range(1,row):
            ans*=(row-col)
            ans//=col
            ansrow.append(ans)


        return ansrow
