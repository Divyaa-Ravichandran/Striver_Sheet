
URL:   https://leetcode.com/problems/max-consecutive-ones/description/

class Solution:
    def findMaxConsecutiveOnes(self, nums: List[int]) -> int:
        maxi=0
        cnt=0
        for i in range(len(nums)):
            if nums[i]==1:
                cnt+=1
            else:
                cnt=0
            
            maxi=max(maxi,cnt)

        return maxi


        #TC: O(N) for traversing
        # SC: O(1) as no space
